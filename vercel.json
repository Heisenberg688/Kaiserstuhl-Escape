{
    "functions": {
        "api/**/*.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "public/(.*)",
            "dest": "/index.php"
        },
        {
            "src": "public/stylesheet/styles.css",
            "dest": "/stylesheet/style.css"
        },
        {
            "src": "public/js/script.js",
            "dest": "/js/script.js"
        }
    ]
}